{
  "name": "dhru-rest-api-framework",
  "type": "project",
  "license": "EULA",
  "homepage": "https://github.com/dhru-com/rest-api-framework",
  "description": "Build REST APIs simply and auto generate postman documents.",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "dhru.com",
      "email": "info@dhru.com"
    }
  ],
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "phpdocumentor/phpdocumentor": "dev-master",
    "squizlabs/php_codesniffer": "^3.0@dev"
  },
  "autoload": {
    "psr-0": {
      "Example": "src/"
    },
    "psr-4": {
      "": "src/"
    },
    "classmap": [
      "src/"
    ]
  }
}
